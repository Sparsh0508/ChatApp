# Dependencies
/node_modules
/.pnp
.pnp.js

# Testing
/coverage

# Production build
/build
/dist

# Local env files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Editor directories and files
.vscode/*
!.vscode/extensions.json
.idea
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# OS files
.DS_Store
Thumbs.db

# Misc
*.pem
*.key
*.crt

# Cache
.cache/
.parcel-cache/
.next/
out/
.tmp/
temp/
*.tsbuildinfo

# Storybook
storybook-static

# Optional npm cache directory
.npm

# Optional eslint cache
.eslintcache

# Optional stylelint cache
.stylelintcache

# Yarn v2 / v3
.yarn/*
!.yarn/patches
!.yarn/releases
!.yarn/plugins
!.yarn/sdks
!.yarn/versions

